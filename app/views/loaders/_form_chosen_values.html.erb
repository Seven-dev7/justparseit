<%= form_with model: loader do |form| %>
  <div class="card">
    <div class="card-body">
      <div class="d-flex">
        <h6 class="card-subtitle mt-1 text-muted">Choisir les clés à récupérer : </h6>
        <p class="ml-1 card-text"><%= form.text_field :chosen_values %> </p>
      </div>
    </div>
    <span class="card-link text-center"><%= form.submit('Valider') %></span>
  </div>
<% end %>
